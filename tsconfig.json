{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "components": ["src/components/*"],
      "screens": ["src/screens/*"]
    },
    "target": "es2016",
    "jsx": "react-native",
    "module": "commonjs",
    "types": ["react-native", "styled-components-react-native"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "extends": "expo/tsconfig.base",
  "includes": ["src"]
}
